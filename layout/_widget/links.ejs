<% if (site.posts.length) { %>
    <div class="widget-wrap links">
        <h3 class="widget-title">友情链接</h3>
        <div class="widget">
            <ul>
                <% for (var i in theme.links) { %>
                    <li>
                        <a href="<%- theme.links[i] %>"><%= i %></a>
                    </li>
                <% } %>
            </ul>
        </div>
    </div>
<% } %>
